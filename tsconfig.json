{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "esnext",
    "lib": ["dom", "dom.iterable"],
    "esModuleInterop": true,
    "declaration": true,
    "composite": true,
    "noEmit": false,
    "jsx": "preserve",
    "emitDeclarationOnly": true,
    "baseUrl": ".",
    "paths": {}
  },
  "exclude": [
    "node_modules"
  ]
}
